{
  "compilerOptions": {
    "rootDir": ".",
    "outDir": "./dist",
    "module": "commonjs",
    "target": "ES2020",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      // "@prisma/*": ["prisma/*"]
    }
  },
  "include": ["src", "prisma/**/*"],
  "exclude": ["node_modules", "dist"]
}
